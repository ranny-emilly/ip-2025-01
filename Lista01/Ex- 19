programa {
    inclua biblioteca Matematica --> mat
    
    funcao inicio() {
        inteiro n, k
        real S = 0.0
        
        leia(n)
        
        se (n <= 1) {
            escreva("Numero invalido!\n")
        } senao {
            para (k = 1; k <= n; k++) {
                S = S + 1.0 / k
            }
          
            escreva(mat.arredondar(S, 6), "\n")
        }
    }
}
