programa {

  inclua biblioteca Matematica--> mat
  funcao inicio() {

    real media, n1, n2, n3

    escreva ("Digite a sua primeira nota\n")
    leia (n1)

    escreva ("Digite a sua segunda nota\n")
    leia (n2)

    escreva ("Ok, agora digite a terceira\n")
    leia (n3)

    media = (n1+n2+n3)/3

    escreva ("media= ", mat.arredondar(media,2))

    se (media>= 6) escreva ("\nAPROVADO!")
    senao escreva ("\nREPROVADO!")


  }
}
