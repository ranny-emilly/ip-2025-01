programa {
    funcao inicio() {
        inteiro x, y, i
        
        leia(x, y)
        
        se (x % 2 == 0) {
            para (i = 0; i < y; i++) {
                escreva(x + (i * 2))
                se (i < y - 1) {
                    escreva(" ")
                }
            }
            escreva("\n")
        } senao {
            escreva("O PRIMEIRO NUMERO NAO E PAR\n")
        }
    }
}
